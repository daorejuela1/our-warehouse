<div class="d-flex justify-content-center m-3">
  <%= link_to("Show Boxes", boxes_path, class: "form-inline btn btn-info mx-2")%>
  <%= link_to("Add Box", new_box_path, class: "form-inline btn btn-success mx-2")%>
</div>
<section class="pricing py-5">
  <div class="container">
    <div class="row">
      <!-- Free Tier -->
      <%= render "tier", name:"Free", price: "0", description: "Max 1 Box Per Team"  %>
      <%= render "tier", name:"Moderate", price: "10", description: "Max 5 Box Per Team"  %>
      <%= render "tier", name:"Unlimited", price: "25", description: "Unlimited Boxes Per Team"  %>
    </div>
  </div>
</section>
